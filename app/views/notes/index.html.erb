<div style="width:600px; margin:auto">
<p id="notice"><%= notice %></p>

<h1>Notes</h1>

<table cellspacing="0" id="categories_table" class="table table-striped table-condensed table-responsive">
  

  <tbody>
    <% @notes.each do |note| %>
      <%= render note %>
    <% end unless @notes.blank? %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_note_path, :class=>'btn btn-primary' %>
</div>
